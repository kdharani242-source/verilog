// Code your design here
module addition(a,b,y);
  input a,b;
  output y;
  assign y=a+b;
endmodule
// Code your testbench here
// or browse Examples
module tb;
  logic a,b;
  logic y;
  addition u1(.a(a),.b(b),.y(y));
  initial
    begin
      $monitor("a=%0b b=%0b y=%0b",a,b,y);
      a='b1;b='b0;
    end
endmodule
