//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int n;
	    scanf("%d",&n);
	    int arr[n];
	    int max=0;
	    for(int i=0;i<n;i++){
	        scanf("%d",&arr[i]);
	        if(arr[i]>max){
	            max=arr[i];
	        }
	    }
	    printf("%d\n",max);
	}

}

//python
# cook your dish here
def solve():
    n=int(input())
    li=list(map(int,input().split()))
    print(max(li))
t=int(input())
for j in range(t):
    solve()
        
