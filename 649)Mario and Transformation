//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int x;
	    scanf("%d",&x);
	    if(x%3==0){
	        printf("normal\n");
	    }
	    else if(x%3==1){
	        printf("huge\n");
	    }
	    else{
	        printf("small\n");
	    }
	}

}

//python
# cook your dish here
t=int(input())
for i in range(t):
    x=int(input())
    if(x%3==0):
        print("normal")
    elif(x%3==1):
        print("huge")
    else:
        print("small")
